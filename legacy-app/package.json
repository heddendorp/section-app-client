{
  "name": "tumi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "build:staging": "ng build --configuration=staging",
    "build:stats": "ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy": "firebase use default && yarn run build:prod",
    "predeploy:full": "yarn run predeploy",
    "predeploy:staging": "firebase use staging && yarn run build:staging",
    "deploy:full": "yarn run upload:all",
    "deploy:staging": "yarn run upload:all",
    "postdeploy:staging": "firebase use default",
    "deploy": "yarn run upload",
    "upload": "node upload.js hosting",
    "upload:all": "node upload.js",
    "precommit": "lint-staged",
    "prettify": "prettier --write ./**/*.{json,css,scss,md,js,ts}",
    "prebundle-report": "yarn run build:stats",
    "prebundle-report:next": "yarn run build:stats",
    "bundle-report": "webpack-bundle-analyzer dist/tumi/stats-es5.json",
    "bundle-report:next": "webpack-bundle-analyzer dist/tumi/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular-buddies/prettier": "^1.0.0-alpha.3",
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/fire": "^5.3.0",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/material-moment-adapter": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/pwa": "^0.803.5",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "@ngxs/devtools-plugin": "^3.5.1",
    "@ngxs/form-plugin": "^3.5.1",
    "@ngxs/router-plugin": "^3.5.1",
    "@ngxs/storage-plugin": "^3.5.1",
    "@ngxs/store": "^3.5.1",
    "firebase": "~7.6.1",
    "first-input-delay": "^0.1.3",
    "hammerjs": "^2.0.8",
    "jspdf": "^1.5.3",
    "moment": "^2.24.0",
    "ngx-markdown": "^8.1.1",
    "ngx-quicklink": "^0.1.4",
    "parallax-js": "^3.1.0",
    "qrcode": "1.4.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.803.5",
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "~12.7.7",
    "codelyzer": "^5.1.1",
    "firebase-tools": "^7.3.0",
    "fuzzy": "^0.1.3",
    "git-last-commit": "^1.0.0",
    "husky": "^3.0.4",
    "inquirer": "^6.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.3.0",
    "prettier": "^1.12.1",
    "protractor": "~5.4.0",
    "rxjs-spy": "^7.5.1",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": ">=3.1 < 3.6",
    "webpack-bundle-analyzer": "^3.5.1"
  }
}
